// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

void fun(int count)
{
    // static int count=0;
    if(count==5)
    {
        return;
    }
    
    cout<<"Vandit"<<endl;
    count++;
    fun(count);
}
int main() {
    int count=0;
    fun(count);

    return 0;
}
