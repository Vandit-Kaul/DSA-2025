class Solution {
public:
    vector<int> sortArray(vector<int>& nums) {
        int size = nums.size();

        for (int i = 0; i < size - 1; i++) {
            int ind = i;
            for (int j = i + 1; j < size; j++) {
                if(nums[j]<nums[ind])
                ind = j;
            }
            swap(nums[i], nums[ind]);
        }
        return nums;
    }
};
